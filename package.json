{
  "name": "menu-porn-api",
  "version": "0.0.1",
  "private": true,
  "description": "This is api for site of menu porn.",
  "scripts": {
    "start": "npx nodemon ./src/application mode=dev",
    "test": "npx mocha ./spec/**/*.spec.js mode=test",
    "build": "npm run babel && npm run minify",
    "babel": "npx babel src --out-dir build",
    "minify": "npx uglifyjs-folder build -o build/ -c -m -e -x .js",
    "seed:undo": "npx sequelize db:seed:undo:all",
    "seed": "npx sequelize db:seed:all",
    "lint": "npx eslint ./"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "extension": [
      ".js"
    ],
    "exclude": [
      "src/**/*.html",
      "**/*.spec.js",
      "ci_scripts",
      "build",
      "spec"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codermarcos/menu-porn-api.git"
  },
  "keywords": [
    "Menu-Porn",
    "MenuPorn",
    "Menu",
    "Porn"
  ],
  "author": {
    "name": "codermarcos",
    "email": "coder.marcos@gmail.com",
    "url": "https://github.com/codermarcos"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/codermarcos/menu-porn-api/issues"
  },
  "homepage": "https://github.com/codermarcos/menu-porn-api#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.0",
    "mysql2": "^1.6.4",
    "phantom": "^6.0.3",
    "sequelize": "^4.42.0",
    "superagent": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^5.12.1",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-mocha": "^5.2.1",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.3",
    "nodemon": "^1.18.9",
    "npx": "^10.2.0",
    "nyc": "^13.1.0",
    "rewire": "^4.0.1",
    "sequelize-cli": "^5.4.0",
    "sinon": "^7.2.3"
  }
}
